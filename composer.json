{
  "type": "project",
  "require": {
    "christianbarkowsky/contao-tiny-compress-images": "^2.1",
    "codefog/contao-news_categories": "^4.0",
    "contao/calendar-bundle": "5.6.*",
    "contao/comments-bundle": "5.6.*",
    "contao/conflicts": "*@dev",
    "contao/faq-bundle": "5.6.*",
    "contao/listing-bundle": "5.6.*",
    "contao/manager-bundle": "5.6.*",
    "contao/news-bundle": "5.6.*",
    "menatwork/contao-multicolumnwizard-bundle": "^3.5",
    "mixpanel/mixpanel-php": "^2.6",
    "oveleon/contao-cookiebar": "^2.3",
    "terminal42/contao-conditionalformfields": "^3.1",
    "terminal42/contao-mp_forms": "^5.2",
    "terminal42/notification_center": "^2.5"
  },
  "require-dev": {
    "contao/manager-plugin": "^2.0"
  },
  "extra": {
    "public-dir": "public",
    "contao-component-dir": "assets",
    "contao-manager-plugin": "App\\ContaoManager\\ContaoManagerPlugin"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "@php vendor/bin/contao-setup"
    ],
    "post-update-cmd": [
      "@php vendor/bin/contao-setup"
    ]
  },
  "config": {
    "allow-plugins": {
      "contao-components/installer": true,
      "contao/manager-plugin": true,
      "contao-community-alliance/composer-plugin": true,
      "php-http/discovery": true
    }
  }
}
